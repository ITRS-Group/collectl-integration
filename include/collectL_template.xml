<sampler name="CollectL CPU">
    <sampleInterval>
        <data>20</data>
    </sampleInterval>
    <fileTrigger>
        <var-filename>
            <data>path to your file here</path></data>
        </var-filename>
    </fileTrigger>
    <plugin>
        <ix-ma>
            <adapters>
                <text>
                    <source>
                        <data></data>
                    </source>
                    <filename>
                        <data>path to your file here</data>
                    </filename>
                    <fieldDelimiter>
                        <data>&quot;,&quot;</data>
                    </fieldDelimiter>
                </text>
            </adapters>
            <trackers>
                <fieldValue>
                    <name>
                        <data>cpu</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_1</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>user</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_2</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>nice</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_3</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>sys</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_4</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>wait</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_5</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>irq</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_6</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>soft</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_7</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>steal</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_8</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>idle</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_9</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>intrptTot</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_10</data>
                    </fieldName>
                </fieldValue>
                <fieldValue>
                    <name>
                        <data>timeOfDay</data>
                    </name>
                    <filter>
                        <data>all</data>
                    </filter>
                    <fieldName>
                        <data>field_11</data>
                    </fieldName>
                </fieldValue>
            </trackers>
            <table>
                <tableColumnLabels>
                    <label>
                        <data>CPU</data>
                    </label>
                    <label>
                        <data>userP</data>
                    </label>
                    <label>
                        <data>niceP</data>
                    </label>
                    <label>
                        <data>sysP</data>
                    </label>
                    <label>
                        <data>waitP</data>
                    </label>
                    <label>
                        <data>irqP</data>
                    </label>
                    <label>
                        <data>softP</data>
                    </label>
                    <label>
                        <data>stealP</data>
                    </label>
                    <label>
                        <data>idleP</data>
                    </label>
                    <label>
                        <data>intrptTot</data>
                    </label>
                    <label>
                        <data>time</data>
                    </label>
                </tableColumnLabels>
                <rowSeedFilter>
                    <rowSeedSelection>
                        <data>all</data>
                    </rowSeedSelection>
                    <rowSeedCells>
                        <cell>
                            <data>$cpu</data>
                        </cell>
                        <cell>
                            <data>$user</data>
                        </cell>
                        <cell>
                            <data>$nice</data>
                        </cell>
                        <cell>
                            <data>$sys</data>
                        </cell>
                        <cell>
                            <data>$wait</data>
                        </cell>
                        <cell>
                            <data>$irq</data>
                        </cell>
                        <cell>
                            <data>$soft</data>
                        </cell>
                        <cell>
                            <data>$steal</data>
                        </cell>
                        <cell>
                            <data>$idle</data>
                        </cell>
                        <cell>
                            <data>$intrptTot</data>
                        </cell>
                        <cell>
                            <data>$timeOfDay</data>
                        </cell>
                    </rowSeedCells>
                </rowSeedFilter>
            </table>
        </ix-ma>
    </plugin>
</sampler>